class Solution {
    public static void main(String[] args) {
        // Write your code here
        int a=1;
        int b=2;
      System.out.println("a is "+a + " and b is "+b);
      a=a+b;
      b=a-b;
      a=a-b;
      System.out.println("after swaping,a is"+a +"and b is "+b);
    }
          }
